{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  data() {\n    return {\n      nameApp: 'LogIn',\n      usuarioIngresado: {\n        nombreUsuario: '',\n        contrasenia: ''\n      },\n      usuarios: []\n      /*usuarios: [\n          {\n              nombreUsuario: 'juan',\n              contrasenia: 1234\n          },\n          {\n              nombreUsuario: 'pablo',\n              contrasenia: 2415\n          },\n          {\n              nombreUsuario: 'ana',\n              contrasenia: 7845\n          }\n      ]*/\n    };\n  },\n\n  methods: {\n    //creamos un objetos con funciones (métodos)\n    getUsuarios() {\n      fetch('http://10.32.0.38:5000/usuarios', {\n        method: \"GET\",\n        headers: {\n          'Content-Type': 'application/json'\n        }\n      }).then(resp => resp.json()).then(data => {\n        let usuarios = data.usuarios;\n        console.log(usuarios);\n        usuarios.forEach(usuario => {\n          this.usuarios.push(usuario);\n        });\n        //this.articles.push(...data)\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n    comprobarUsuario: function (e) {\n      e.preventDefault();\n      var flag = false;\n      console.log(this.usuarioIngresado);\n      this.usuarios.forEach(usuario => {\n        if (this.usuarioIngresado.nombreUsuario == usuario.nombreUsuario && this.usuarioIngresado.contrasenia == usuario.contrasenia) {\n          console.log('Acceso concedido');\n          flag = true;\n          window.location.replace(\"http://localhost:8080\");\n          return;\n        }\n      });\n      if (flag == false) console.log('Acceso denegado');\n    }\n  },\n  created() {\n    this.getUsuarios();\n  }\n};","map":{"version":3,"names":["data","nameApp","usuarioIngresado","nombreUsuario","contrasenia","usuarios","methods","getUsuarios","fetch","method","headers","then","resp","json","console","log","forEach","usuario","push","catch","error","comprobarUsuario","e","preventDefault","flag","window","location","replace","created"],"sources":["/Users/chiara/Documents/TpGrupalWeb/frontend/src/components/UserLogin.vue"],"sourcesContent":["<template>\n    <div class=\"container  mt-4\">\n        <div id=\"logInApp\" class=\"col-sm-8 col-sm-offset-2\">\n            <h1>{{nameApp}}</h1>\n            \n            <form v-on:submit=\"comprobarUsuario\">\n                <input type=\"text\" v-model=\"usuarioIngresado.nombreUsuario\" class=\"form-control\">\n                <input type=\"password\" v-model=\"usuarioIngresado.contrasenia\" class=\"form-control\">\n                <input type=\"submit\" value=\"Ingresar\" class=\"btn btn-block btn-success\">\n            </form>\n            <br>\n            \n        </div>\n    </div>\n</template>\n\n<script>\nexport default {\n    data() {\n                return {\n                    nameApp: 'LogIn',\n\n                    usuarioIngresado: {\n                        nombreUsuario:'',\n                        contrasenia:''\n                    },\n                    usuarios: []\n                    /*usuarios: [\n                        {\n                            nombreUsuario: 'juan',\n                            contrasenia: 1234\n                        },\n                        {\n                            nombreUsuario: 'pablo',\n                            contrasenia: 2415\n                        },\n                        {\n                            nombreUsuario: 'ana',\n                            contrasenia: 7845\n                        }\n                    ]*/\n                }\n            },\n            methods: { //creamos un objetos con funciones (métodos)\n                getUsuarios(){\n                    fetch('http://10.32.0.38:5000/usuarios',{\n                        method:\"GET\",\n                        headers:{\n                            'Content-Type': 'application/json'\n                        }\n                    })\n                    .then(resp => resp.json())\n                    .then(data => {\n                    let usuarios = data.usuarios;\n                    console.log(usuarios);\n                    usuarios.forEach(usuario => {this.usuarios.push(usuario)})\n                    //this.articles.push(...data)\n                    })\n                    .catch(error =>{\n                        console.log(error);\n                    })\n                },\n\n                comprobarUsuario: function(e){\n                    e.preventDefault();\n                    var flag = false;\n                    console.log(this.usuarioIngresado);\n                    this.usuarios.forEach(usuario => {\n                        if(this.usuarioIngresado.nombreUsuario == usuario.nombreUsuario && \n                        this.usuarioIngresado.contrasenia == usuario.contrasenia){\n                            console.log('Acceso concedido');\n                            flag = true;\n                            window.location.replace(\"http://localhost:8080\");\n                            return;\n                        }\n                    });\n\n                    if(flag==false)\n                        console.log('Acceso denegado');\n                },\n            },\n            created(){\n                this.getUsuarios();\n            }\n}\n</script>\n\n<style>\n.container div form {\n    display:flex;\n    justify-content:center;\n    width: 100vw;\n}\n\n    .container div form input{\n        width: 100vw;\n    }\n</style>\n"],"mappings":";AAiBA,eAAe;EACXA,IAAIA,CAAA,EAAG;IACK,OAAO;MACHC,OAAO,EAAE,OAAO;MAEhBC,gBAAgB,EAAE;QACdC,aAAa,EAAC,EAAE;QAChBC,WAAW,EAAC;MAChB,CAAC;MACDC,QAAQ,EAAE;MACV;;;;;;;;;;;;;;IAcJ;EACJ,CAAC;;EACDC,OAAO,EAAE;IAAE;IACPC,WAAWA,CAAA,EAAE;MACTC,KAAK,CAAC,iCAAiC,EAAC;QACpCC,MAAM,EAAC,KAAK;QACZC,OAAO,EAAC;UACJ,cAAc,EAAE;QACpB;MACJ,CAAC,EACAC,IAAI,CAACC,IAAG,IAAKA,IAAI,CAACC,IAAI,CAAC,CAAC,EACxBF,IAAI,CAACX,IAAG,IAAK;QACd,IAAIK,QAAO