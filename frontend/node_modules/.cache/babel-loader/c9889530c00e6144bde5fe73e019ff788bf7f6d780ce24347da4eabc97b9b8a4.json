os',{\r\n                        method:\"GET\",\r\n                        headers:{\r\n                            'Content-Type': 'application/json'\r\n                        }\r\n                    })\r\n                    .then(resp => resp.json())\r\n                    .then(data => {\r\n                    let usuarios = data.usuarios;\r\n                    console.log(usuarios);\r\n                    usuarios.forEach(usuario => {this.usuarios.push(usuario)})\r\n                    //this.articles.push(...data)\r\n                    })\r\n                    .catch(error =>{\r\n                        console.log(error);\r\n                    })\r\n                },\r\n\r\n                comprobarUsuario: function(e){\r\n                    e.preventDefault();\r\n                    var flag = false;\r\n                    console.log(this.usuarioIngresado);\r\n                    this.usuarios.forEach(usuario => {\r\n                        if(this.usuarioIngresado.nombreUsuario == usuario.nombreUsuario && \r\n                        this.usuarioIngresado.contrasenia == usuario.contrasenia){\r\n                            console.log('Acceso concedido');\r\n                            flag = true;\r\n                            window.location.replace(\"http://localhost:8080\");\r\n                            return;\r\n                        }\r\n                    });\r\n\r\n                    if(flag==false)\r\n                        console.log('Acceso denegado');\r\n                },\r\n            },\r\n            created(){\r\n                this.getUsuarios();\r\n            }\r\n}\r\n</script>\r\n\r\n<style>\r\n.log{\r\n    color:black;\r\n}\r\n.container{\r\n    display:flex;\r\n    justify-content:center;\r\n}\r\n\r\n.container div form {\r\n    display:block;\r\n    width: 100%;\r\n    margin: 1vh 5vw;\r\n}\r\n\r\n    .container div form input {\r\n        display:flex;\r\n        width: 80%;\r\n        justify-content:center;\r\n        align-items:center;\r\n    }\r\n</style>\r\n"],"mappings":";AAiBA,eAAe;EACXA,IAAIA,CAAA,EAAG;IACK,OAAO;MACHC,OAAO,EAAE,OAAO;MAEhBC,gBAAgB,EAAE;QACdC,aAAa,EAAC,EAAE;QAChBC,WAAW,EAAC;MAChB,CAAC;MACDC,QAAQ,EAAE;MACV;;;;;;;;;;;;;;IAcJ;EACJ,CAAC;;EACDC,OAAO,EAAE;IAAE;IACPC,WAAWA,CAAA,EAAE;MACTC,KAAK,CAAC,iCAAiC,EAAC;QACpCC,MAAM,EAAC,KAAK;QACZC,OAAO,EAAC;UACJ,cAAc,EAAE;QACpB;MACJ,CAAC,EACAC,IAAI,CAACC,IAAG,IAAKA,IAAI,CAACC,IAAI,CAAC,CAAC,EACxBF,IAAI,CAACX,IAAG,IAAK;QACd,IAAIK,QAAO,GAAIL,IAAI,CAACK,QAAQ;QAC5BS,OAAO,CAACC,GAAG,CAACV,QAAQ,CAAC;QACrBA,QAAQ,CAACW,OAAO,CAACC,OAAM,IAAK;UAAC,IAAI,CAACZ,QAAQ,CAACa,IAAI,CAACD,OAAO,CAAC;QAAA,CAAC;QACzD;MACA,CAAC,EACAE,KAAK,CAACC,KAAI,IAAI;QACXN,OAAO,CAACC,GAAG,CAACK,KAAK,CAAC;MACtB,CAAC;IACL,CAAC;IAEDC,gBAAgB,EAAE,SAAAA,CAASC,CAAC,EAAC;MACzBA,CAAC,CAACC,cAAc,CAAC,CAAC;MAClB,IAAIC,IAAG,GAAI,KAAK;MAChBV,OAAO,CAACC,GAAG,CAAC,IAAI,CAACb,gBAAgB,CAAC;MAClC,IAAI,CAACG,QAAQ,CAACW,OAAO,CAACC,OAAM,IAAK;QAC7B,IAAG,IAAI,CAACf,gBAAgB,CAACC,aAAY,IAAKc,OAAO,CAACd,aAAY,IAC9D,IAAI,CAACD,gBAAgB,CAACE,WAAU,IAAKa,OAAO,CAACb,WAAW,EAAC;UACrDU,OAAO,CAACC,GAAG,CAAC,kBAAkB,CAAC;UAC/BS,IAAG,GAAI,IAAI;UACXC,MAAM,CAACC,QAAQ,CAACC,OAAO,CAAC,uBAAuB,CAAC;UAChD;QACJ;MACJ,CAAC,CAAC;MAEF,IAAGH,IAAI,IAAE,KAAK,EACVV,OAAO,CAACC,GAAG,CAAC,iBAAiB,CAAC;IACtC;EACJ,CAAC;EACDa,OAAOA,CAAA,EAAE;IACL,IAAI,CAACrB,WAAW,CAAC,CAAC;EACtB;AACZ"},"metadata":{},"sourceType":"module","externalDependencies":[]}